{
  "name": "@sparkpost/libby-react",
  "version": "0.0.51",
  "description": "Libby is an open source tool for React component development",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/api/index.js",
  "types": "src/api/types.d.js",
  "bin": {
    "libby": "bin/cli.js"
  },
  "scripts": {
    "start:cli": "rollup -w --config rollup.config.ts",
    "build:cli": "rollup --config rollup.config.ts",
    "format": "prettier --write '**/*.{js,md}'",
    "lint": "eslint .",
    "build:ci": "cd example && npm ci && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js}": [
      "eslint --fix"
    ],
    "**/*.{js,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparkPost/libby-react.git"
  },
  "author": "SparkPost",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SparkPost/libby-react/issues"
  },
  "homepage": "https://github.com/SparkPost/libby-react#readme",
  "dependencies": {
    "@styled-system/css": "^5.1.5",
    "@sweatpants/box": "0.0.6",
    "@sweatpants/inline": "0.0.6",
    "@sweatpants/screenreaderonly": "0.0.6",
    "@sweatpants/stack": "0.0.6",
    "@sweatpants/theme": "0.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "chalk": "^5.0.1",
    "esbuild": "^0.14.43",
    "events": "^3.3.0",
    "find-up": "^6.3.0",
    "meow": "^10.1.2",
    "react-element-to-jsx-string": "^15.0.0",
    "react-error-boundary": "^3.1.4",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.5",
    "vite": "^2.9.12",
    "vite-plugin-glob": "^0.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/react": "^18.0.12",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^10.5.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "rollup": "^2.75.6"
  },
  "engines": {
    "npm": "8.12.1"
  }
}
