{
  "name": "@sparkpost/libby-react",
  "version": "0.0.32",
  "description": "Libby is an open source tool for React component development",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/api/index.js",
  "types": "src/api/types.d.js",
  "bin": {
    "libby": "bin/cli.js"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,md}'",
    "lint": "eslint .",
    "build:ci": "cd example && npm ci && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js}": [
      "eslint --fix"
    ],
    "**/*.{js,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SparkPost/libby-react.git"
  },
  "author": "SparkPost",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SparkPost/libby-react/issues"
  },
  "homepage": "https://github.com/SparkPost/libby-react#readme",
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@reach/router": "^1.3.4",
    "@styled-system/css": "^5.1.5",
    "@sweatpants/box": "0.0.5",
    "@sweatpants/button": "0.0.5",
    "@sweatpants/inline": "0.0.5",
    "@sweatpants/screenreaderonly": "0.0.5",
    "@sweatpants/stack": "0.0.5",
    "@sweatpants/theme": "0.0.5",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "css-loader": "^5.0.0",
    "error-overlay-webpack-plugin": "^0.4.2",
    "file-loader": "^6.2.0",
    "find-up": "^5.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.3.1",
    "json-parse-better-errors": "^1.0.2",
    "meow": "^7.1.1",
    "open": "^7.3.0",
    "page-bus": "^3.0.1",
    "query-string": "^6.13.6",
    "react-element-to-jsx-string": "^14.3.2",
    "react-syntax-highlighter": "^15.4.3",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0",
    "webpackbar": "^5.0.0-3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webpack": "^4.46.0"
  }
}
